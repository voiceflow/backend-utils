  "author": "Voiceflow",
  "bugs": {
    "url": "https://github.com/voiceflow/backend-utils/issues"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@types/bluebird": "^3.5.33",
    "@types/chai": "^4.2.16",
    "@types/express": "^4.17.11",
    "@types/ioredis": "^4.26.4",
    "@types/lodash": "^4.14.168",
    "@types/luxon": "^1.26.4",
    "@types/sinon": "^10.0.0",
    "@voiceflow/common": "7.2.0",
    "@voiceflow/logger": "1.6.0",
    "@voiceflow/verror": "^1.1.0",
    "bluebird": "^3.7.2",
    "chai": "^4.3.4",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "http-status": "^1.4.2",
    "lodash": "^4.17.11",
    "luxon": "^1.21.3",
    "rate-limiter-flexible": "^2.2.2",
    "sinon": "^10.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/axios": "^0.14.0",
    "@types/chai-as-promised": "^7.1.4",
    "@types/mocha": "^8.2.2",
    "@types/supertest": "^2.0.11",
    "@voiceflow/commitlint-config": "1.1.0",
    "@voiceflow/eslint-config": "3.5.0",
    "@voiceflow/git-branch-check": "1.2.0",
    "@voiceflow/prettier-config": "1.0.5",
    "@voiceflow/tsconfig": "1.1.0",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "chai-as-promised": "^7.1.1",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "depcheck": "^1.4.0",
    "eslint": "^7.32.0",
    "eslint-output": "^3.0.1",
    "express-validator": "^6.10.1",
    "fixpack": "^4.0.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "mocha": "^8.3.2",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "ts-mocha": "^8.0.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.2.4"
  },
  "files": [
    "build/**"
  ],
  "homepage": "https://github.com/voiceflow/backend-utils#readme",
  "keywords": [
    "utilities"
  ],
  "license": "ISC",
  "main": "build/index.js",
  "peerDependencies": {
    "express-validator": "^6.3.0"
  },
  "prettier": "@voiceflow/prettier-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voiceflow/backend-utils.git"
  },
  "scripts": {
    "build": "yarn clean && ttsc --project ./tsconfig.build.json",
    "clean": "rimraf build",
    "commit": "cz",
    "eslint-output": "eslint-output",
    "lint": "eslint \"**/*.{js,ts}\"",
    "lint:fix": "yarn lint --fix",
    "lint:output": "yarn run eslint-output --quiet \"**/*.{js,ts}\"",
    "lint:quiet": "yarn lint --quiet",
    "lint:report": "yarn lint:output",
    "prepare": "npm run build",
    "test": "yarn test:run",
    "test:dependencies": "depcheck",
    "test:integration": "NODE_ENV=test nyc --report-dir=nyc_coverage_integration ts-mocha --paths --config ./config/tests/.mocharc.yml 'tests/**/*.it.ts'",
    "test:run": "NODE_ENV=test nyc ts-mocha --paths --config ./config/tests/.mocharc.yml 'tests/**/*.{unit,it}.ts'",
    "test:single": "NODE_ENV=test ts-mocha --paths --config ./config/tests/.mocharc.yml",
    "test:unit": "NODE_ENV=test nyc --report-dir=nyc_coverage_unit ts-mocha --paths --config ./config/tests/.mocharc.yml 'tests/**/*.unit.ts'"
  },
  "types": "build/index.d.ts"
}
